    "access": "public"
  },
  "devDependencies": {
    "@dotenv-cloak/tsconfig": "workspace:0.0.0",
    "tsup": "5.12.8",
    "typescript": "4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonycalsin/dotenv-cloak.git"
  },
  "bugs": {
